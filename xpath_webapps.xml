<fingerprints>

<!-- Joomla! "Joomla!" version Status: Development -->
    <fingerprint>
        <get path="/administrator/"/>
        <get path="/"/>
        <test xpath="//html/head/meta[@name='generator']/@content" regex="(Joomla\!).*(\d+\.\d+).*(Open Source Content Management)">
            <param pos="0" name="vendor" value="Joomla!"/>
            <param pos="0" name="product" value="Joomla!"/>
            <param pos="1" name="version"/>
        </test>
    </fingerprint>



<!-- See https://community.rapid7.com/thread/5332 -->
<!-- Drupal "Drupal" version Status: Development -->
    <fingerprint>
        <get path="/"/>
        <test xpath="//descendant-or-self::node()" regex="X-Generator:.*Drupal\s(\d+).*\(http://drupal\.org\)">
            <param pos="0" name="vendor" value="Drupal"/>
            <param pos="0" name="product" value="Drupal"/>
            <param pos="1" name="version"/>
        </test>
    </fingerprint>


