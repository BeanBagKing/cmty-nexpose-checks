<Vulnerability id="cmty-telnet-default-account-modem1-password-modem1" published="2017-11-10" added="2017-11-10" modified="2017-11-10" version="1.0">
  <name>Default Telnet account: modem1 password "<![CDATA[modem1]]>"</name>
  <severity>10</severity>
  <cvss>(AV:N/AC:L/Au:N/C:C/I:C/A:C)</cvss>
  <Tags>
    <tag>Default Account</tag>
    <tag>Telnet</tag>
  </Tags>
  <AlternateIds>
